<%- include('_header')%>

<!--================login Banner Area =================-->
<section class="login_banner_area">
  <div class="container">
    <form class="contact_form" action="#" method="post">
      <h2>Welcome</h2>
      <div class="contact_form_text">
        <div class="form-group form-group_icon">
          <i class="fa fa-user"></i>
          <input
            type="email"
            class="form-control"
            id="email"
            name="email"
            placeholder="Enter your email"
          />
        </div>
        <div class="form-group form-group_icon">
          <i class="fa fa-lock"></i>
          <input
            type="password"
            class="form-control"
            id="password"
            name="password"
            placeholder="Enter your password"
          />
        </div>
        <p class="forgot-password"><a href="/forgot">Forgot password?</a></p>
        <div class="submit_btn_disable_reg disible-control"></div>
        <button type="submit" value="submit" class="btn submit_btn">
          Login
        </button>
      </div>
      <p>Don't have an account? <a href="/signup">create one</a></p>
    </form>
  </div>
</section>
<!--================login Banner Area =================-->
<script src="/js/login.js"></script>
<%- include('_footer')%>
